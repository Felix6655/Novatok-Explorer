generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model PluginIdea {
    id          String   @id @default(cuid())
    title       String
    category    String
    description String
    handle      String?
    status      String   @default("new")
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}
